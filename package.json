{
  "name": "figma-icons-getter",
  "version": "0.0.0",
  "description": "A simple solution to get component set icons from Figma",
  "type": "module",
  "bin": "dist/cli.js",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "scripts": {
    "prepare": "tsc",
    "start": "node --import tsx --env-file=.env src/cli.ts --file mfiglkk3bXQwetaRsftZQz --out ./playground"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcusick93/figma-icons-getter.git"
  },
  "keywords": [
